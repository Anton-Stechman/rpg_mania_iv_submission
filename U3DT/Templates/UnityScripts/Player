    }

    private void Start() {
        // execute on game start
        control = new Motion(MoveSpeed, MaxSpeed, Mass)
    }

    private void FixedUpdate() {
        // physics
    }

    private void Update() {
        // Other
    }

    private void LateUpdate() {
        // camera
    }
}